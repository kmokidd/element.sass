                                            a.words.showLine(href="") 行业分类
                                        li.last
                                            a.words.showLine(href="") 国家地区
                                li.admin
                                    a.words(href="") 管理员操作
                                li.contentM
                                    a.words(href="") 内容管理
                        div.user.clearfix
                            ul.user-setting
                                li
                                    a.word.clearfix listen1999
                                    i.icon-arrow-down.clearfix
                                    ul.second
                                        li.last
                                            a.word(href="") 退出登录
                div.header-two
                    div.header-two-inner
                        nav.submain-nav
                            ul.breadcrumb
                                - var breadcrumb = ['Home', 'Publish', 'New Entry', 'Users'];
                                    li.Home
                                        a(href='')= breadcrumb[0]
                                        i.divider &gt;
                                    li.Publish
                                        a(href='')= breadcrumb[1]
                                        i.divider &gt;
                                    li.NE
                                        a(href='')= breadcrumb[2]
                                        i.divider &gt;
                                    li.Users
                                        a.active(href='')= breadcrumb[3]
